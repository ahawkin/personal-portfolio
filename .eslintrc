{
    "plugins": [
      "mocha"
    ],
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-underscore-dangle": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/no-danger": 0,
      "linebreak-style": 0
    },
    "env": {
      "browser": true,
      "mocha": true
    }
  }